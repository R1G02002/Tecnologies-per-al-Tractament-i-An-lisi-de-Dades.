   ]
  },
  {
   "cell_type": "code",
   "execution_count": 64,
   "id": "7597a4f8-7046-4bf7-b402-9d3023d20b81",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
