   "cell_type": "code",
   "execution_count": 63,
   "id": "f9fe9e72-f918-490d-a2ea-962153b98916",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
